{
    "name": "product-documentation",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "scully": "cross-env BUILD_ENV=develop npx scully --scanRoutes",
        "scully:routes": "npx scully --scanRoutes",
        "scully:serve": "npx scully serve",
        "scully:prod": "npm run build && npm run scully:routes",
        "scully:start": "npm run build && npm run scully",
        "prepare": "husky install",
        "lint": "ng lint",
        "prettier": "prettier \"src/**/*.{ts,html,scss}\" --check",
        "prettier:format": "prettier \"src/**/*.{ts,html,scss}\" --write"
    },
    "private": true,
    "prettier": "./.prettierrc.js",
    "dependencies": {
        "@angular/animations": "~13.3.0",
        "@angular/common": "~13.3.0",
        "@angular/compiler": "~13.3.0",
        "@angular/core": "~13.3.0",
        "@angular/elements": "^13.4.0",
        "@angular/forms": "~13.3.0",
        "@angular/platform-browser": "~13.3.0",
        "@angular/platform-browser-dynamic": "~13.3.0",
        "@angular/router": "~13.3.0",
        "@scullyio/init": "^2.1.29",
        "@scullyio/ng-lib": "^2.1.0",
        "@scullyio/scully": "^2.1.0",
        "@scullyio/scully-plugin-puppeteer": "^2.1.0",
        "rxjs": "~7.5.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^13.3.11",
        "@angular-eslint/builder": "13.5.0",
        "@angular-eslint/eslint-plugin": "13.5.0",
        "@angular-eslint/eslint-plugin-template": "13.5.0",
        "@angular-eslint/schematics": "13.5.0",
        "@angular-eslint/template-parser": "13.5.0",
        "@angular/cli": "^13.3.11",
        "@angular/compiler-cli": "~13.3.0",
        "@types/jasmine": "~3.10.0",
        "@types/jsdom": "^16.2.14",
        "@types/node": "^12.11.1",
        "@typescript-eslint/eslint-plugin": "5.27.1",
        "@typescript-eslint/eslint-plugin-tslint": "^5.58.0",
        "@typescript-eslint/parser": "5.27.1",
        "cross-env": "^7.0.3",
        "eslint": "^8.38.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsdoc": "^40.3.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-sonarjs": "^0.18.0",
        "husky": "^8.0.0",
        "jasmine-core": "~4.0.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.1.0",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "lint-staged": "^13.2.1",
        "prettier": "^2.8.7",
        "typescript": "~4.6.2"
    },
    "lint-staged": {
        "src/**/*.{js,ts}": "eslint",
        "src/**/*.{js,ts,css,scss,html}": "prettier --write"
    }
}
