{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "API Documentation",
  "colors": {
    "primary": "#d5d5d5",
    "light": "#d5d5d5",
    "dark": "#363636"
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "API Reference",
        "anchors": [
          {
            "anchor": "IAM",
            "openapi": {
              "source": "openapi/iam-openapi.yaml",
              "directory": "api-reference/iam"
            }
          }
        ]
      }
    ]
  },
  "appearance": {
    "default": "light"
  },
  "seo": {
    "metatags": {
      "robots": "noindex",
      "og:image": "/"
    }
  }
}